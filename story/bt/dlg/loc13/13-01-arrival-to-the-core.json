    {
      "actor": "",
      "name": "Arrival to the Core",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": "Прибытие в Ядро"
        }
      ]
    },
    {
      "actor": "Lexi",
      "name": "This place... it's silent. Like time gave up.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Buzz",
      "name": "Chrono Core. The end and beginning of all time.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Lexi",
      "name": "Where is the Time Lord?",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "TimeLord",
      "name": "I have been waiting... I was waiting... I will be... have... been—",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Buzz",
      "name": "He's breaking. His mind flows across all tenses.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "TimeLord",
      "name": "Time shattered. The Seals undone. The moments bleed.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Lexi",
      "name": "We're here. We’ve brought the Twelve.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "TimeLord",
      "name": "You carry what I can no longer hold.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Buzz",
      "name": "Only someone who walked through time can restore it.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Lexi",
      "name": "Tell us what to do.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "TimeLord",
      "name": "Say what must be said. In the time it belongs.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Lexi",
      "name": "Then we begin.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    }
  ]
}