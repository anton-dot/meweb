    {
      "actor": "Rin",
      "name": "I remember a time I made a mistake.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Lumo",
      "name": "We all did. What happened?",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Rin",
      "name": "I trusted the wrong person.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Rin",
      "name": "He promised help. I believed him.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Lumo",
      "name": "What did he do?",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Rin",
      "name": "He took what I had and disappeared.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Lumo",
      "name": "You felt betrayed.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Rin",
      "name": "Yes. I didn’t see the signs.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Lumo",
      "name": "But you learned.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Rin",
      "name": "I changed after that.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Lumo",
      "name": "I once ignored a warning.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Rin",
      "name": "What happened?",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Lumo",
      "name": "I entered a storm. I got lost for days.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Rin",
      "name": "Did someone find you?",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Lumo",
      "name": "No. I found myself.",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
          "name": ""
        }
      ]
    },
    {
      "actor": "Lumo",
      "name": "Which sentence shows a past mistake?",
      "dialogType": "Normal",
      "translates": [
        {
          "lang": "ru-RU",
