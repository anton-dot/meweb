      "image": "",
      "bg": "/story/oz1/bg/09.png",
      "glowColor": "#AAFFDDFF",
      "bgBlur": "#00445588",
      "dialogType": "Normal",
      "actions": []
    },
    {
      "name": "We’ll find another way.",
      "actor": "Lexi",
      "translates": [
        {
          "lang": "ru-RU",
          "name": "Мы найдём другой способ."
        }
