      "image": "",
      "bg": "/story/oz1/bg/09.png",
      "glowColor": "#AAFFDDFF",
      "bgBlur": "#00445588",
      "dialogType": "Normal",
      "actions": []
    },
    {
      "name": "Again? We already lost Vel!",
      "actor": "Leon",
      "translates": [
        {
          "lang": "ru-RU",
          "name": "Опять? Мы уже потеряли Вэла!"
